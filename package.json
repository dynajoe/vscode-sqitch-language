{
  "name": "sqitch-plan",
  "displayName": "Sqitch Plan Language",
  "description": "Sqitch plan highlighting",
  "version": "0.0.2",
  "publisher": "joeandaverde",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "plan",
